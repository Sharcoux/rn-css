{
  "name": "rn-css",
  "version": "1.11.5",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.js",
      "types": "./dist/*/index.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "prepare": "rm -rf dist && tsc && webpack",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint --fix .",
    "build": "webpack",
    "release": "npm run prepare && release-it",
    "web": "webpack serve --open --mode development"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-native": ">=0.62.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-native": "^0.66.9",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-standard": "^5.0.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.1.2",
    "metro-react-native-babel-preset": "^0.70.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.68.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-web": "^0.17.7",
    "react-test-renderer": "17.0.2",
    "release-it": "^15.6.0",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "files": [
    "dist",
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.[tj]s?(x)": [
      "eslint . --fix",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/Sharcoux/rn-css",
  "author": {
    "name": "FranÃ§ois Billioud",
    "email": "f.billioud@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Sharcoux/rn-css/issues",
    "email": "f.billioud@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Sharcoux/rn-css.git"
  },
  "sideEffects": false,
  "license": "ISC"
